{
  "name": "cash-flow-tracker-api",
  "version": "1.0.0",
  "description": "Backend API for Cash Flow Tracker application with Google Sheets integration",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "validate": "npm run lint",
    "clean": "rm -rf logs/ dist/"
  },
  "keywords": [
    "cash-flow",
    "finance",
    "tracker",
    "api",
    "express",
    "google-sheets",
    "nodejs",
    "backend"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/cash-flow-tracker-api.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/cash-flow-tracker-api/issues"
  },
  "homepage": "https://github.com/tu-usuario/cash-flow-tracker-api#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.11",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^13.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jsdoc": "^4.0.2",
    "nodemon": "^3.0.0",
    "prettier": "^3.1.0"
  }
}
